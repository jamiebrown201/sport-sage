    "migrate": "drizzle-kit migrate",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-rds-data": "^3.700.0",
    "drizzle-orm": "^0.38.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "drizzle-kit": "^0.30.1",
    "typescript": "^5.9.2"
